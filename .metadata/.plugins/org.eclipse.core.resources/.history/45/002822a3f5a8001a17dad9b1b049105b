package model;

public class Gerente extends Funcionario implements Autenticavel{
	
	private int senha;

	@Override
	public double getbonificacao() {
		return this.salario * 0.15;
	}

	@Override
	public void autentica(int senha) {
		
		if(this.senha == senha)System.out.println("Liberado");
		
	}

}
